{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "command-center/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/app/(.*)",
      "dest": "/command-center/$1"
    },
    {
      "src": "/app",
      "dest": "/command-center"
    },
    {
      "src": "/login",
      "dest": "/command-center/login"
    },
    {
      "src": "/api/auth/(.*)",
      "dest": "/command-center/api/auth/$1"
    },
    {
      "src": "/_next/(.*)",
      "dest": "/command-center/_next/$1"
    },
    {
      "handle": "filesystem"
    }
  ]
}