{
  "version": 2,
  "rewrites": [
    {
      "source": "/app",
      "destination": "/command-center"
    },
    {
      "source": "/app/:path*",
      "destination": "/command-center/:path*"
    },
    {
      "source": "/_next/:path*",
      "destination": "/command-center/_next/:path*"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "command-center/package.json",
      "use": "@vercel/next"
    }
  ]
}